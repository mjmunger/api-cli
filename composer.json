{
  "name": "hphio/api-cli",
  "description": "Implements a CLI back-end for managing web apps and APIs",
  "require": {
    "league/container": ">=4.2",
    "ext-pdo": "*",
    "ext-readline": "*",
    "league/climate": "^3.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-0": {
      "hphio\\cli\\": "src"
    },
    "psr-4": {
      "hphio\\cli\\": "src"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Tests\\": "tests"
    },
    "psr-4": {
      "Tests\\": "tests"
    }
  }
}
